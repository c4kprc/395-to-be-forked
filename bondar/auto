for i in {325..335};
do
	mkdir группа$i
        cd группа$i
        mkdir "Управление задачами"
        cd "Управление задачами"
        mkdir "Документация"
	cd Документация
	touch Инструкция.txt
	mkdir "Шаблоны задач" && cd "Шаблоны задач"
	touch Шаблон1.txt
	touch Шаблон2.txt
	cd ../..
        mkdir Код && cd Код
	mkdir Исходники && cd Исходники
	touch main.c && touch utils.c
	cd ..
	mkdir Скрипты && cd Скрипты
	touch setup.sh && touch run.sh
	cd ../..
        mkdir Отчёты && cd Отчёты
	touch Отчет1.doc && touch Отчет2.doc
        cd ../../..
done
